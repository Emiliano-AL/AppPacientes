<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lania.pacientesapp.app" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.lania.pacientesapp.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lania.pacientesapp.app.Pacientes"
            android:label="@string/title_activity_pacientes" >
        </activity>
        <activity
            android:name="com.lania.pacientesapp.app.Glucosa"
            android:label="@string/title_activity_glucosa" >
        </activity>
        <activity
            android:name="com.lania.pacientesapp.app.PresionArterial"
            android:label="@string/title_activity_presion_arterial" >
        </activity>
        <activity
            android:name="com.lania.pacientesapp.app.Historial"
            android:label="@string/title_activity_historial" >
        </activity>
        <activity
            android:name="com.lania.pacientesapp.app.AcercaDe"
            android:label="@string/title_activity_acerca_de"
            android:theme="@android:style/Animation.Dialog">
        </activity>
    </application>

</manifest>
